    {
      "id": "demo-mevxr0df-3ee4",
      "title": "Run App",
      "steps": [
        {
          "action": "openTerminal",
          "terminalId": "titanic"
        },
        {
          "action": "executeTerminalCommand",
          "terminalId": "titanic",
          "command": "clear"
        },
        {
          "action": "executeTerminalCommand",
          "command": "cd /workspaces/streamlit-demo/app",
          "insertTypingMode": "instant",
          "terminalId": "titanic"
        },
        {
          "action": "executeTerminalCommand",
          "command": "uv run streamlit run titanic_app.py",
          "insertTypingMode": "character-by-character",
          "autoExecute": false,
          "terminalId": "titanic"
        }
      ]
    }
  ]
