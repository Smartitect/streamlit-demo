{
  "$schema": "https://demotime.show/demo-time.schema.json",
  "title": "Demo 1 - Basic App",
  "description": "",
  "version": 2,
  "demos": [
    {
      "id": "demo-mf1jetk6-0gxj",
      "title": "Set Up",
      "steps": [
        {
          "action": "deleteFile",
          "path": "app/titanic_app.py"
        },
        {
          "action": "create",
          "path": "app/titanic_app.py",
          "content": "# Empty file!"
        },
        {
          "action": "closeTerminal",
          "terminalId": "titanic"
        },
        {
          "action": "open",
          "path": "app/titanic_app.py"
        }
      ],
      "icons": {
        "start": "",
        "end": ""
      },
      "description": "Setup for the demo."
    },
    {
      "id": "demo-mevulvne-kho1",
      "title": "Notebook",
      "steps": [
        {
          "action": "open",
          "path": "notebooks/titanic_data_exploration.ipynb"
        }
      ]
    },
    {
      "id": "demo-mevus6bv-ve47",
      "title": "Export Notebook to Python",
      "steps": [
        {
          "action": "open",
          "path": "notebooks/data_exploration.ipynb"
        },
        {
          "action": "executeVSCodeCommand",
          "command": "jupyter.exportAsPythonScript"
        }
      ]
    },
    {
      "id": "demo-mevvoe1v-bvtm",
      "title": "Save File Into \"app\" Folder",
      "steps": [
        {
          "action": "executeVSCodeCommand",
          "command": "workbench.action.files.saveAs",
          "args": {
            "Uri": "/workspaces/streamlit-demo/app/titanic_app.py"
          }
        }
      ]
    },
    {
      "id": "demo-mevw4dl8-rr93",
      "title": "Replace Cell Placeholders",
      "steps": [
        {
          "action": "executeVSCodeCommand",
          "command": "editor.action.startFindReplaceAction"
        }
      ]
    },
    {
      "id": "demo-mevut7za-xpdl",
      "title": "Add Streamlit",
      "steps": [
        {
          "action": "replace",
          "path": "app/titanic_app.py",
          "content": "import streamlit as st",
          "position": "1",
          "highlightWholeLine": true,
          "insertTypingMode": "character-by-character",
          "insertTypingSpeed": 10,
          "zoom": 3
        },
        {
          "action": "highlight",
          "position": "1:1",
          "highlightWholeLine": true,
          "path": "app/titanic_app.py"
        }
      ]
    },
    {
